{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {        
    },
    "rules": {
        "member-access": false,
        "object-literal-sort-keys": false,
        "prefer-const": false,
        "ordered-imports": false,
        "jsx-no-lambda": false,
        "no-empty": false   
    },
    "rulesDirectory": []
}